{
  "name": "HDObject",
  "version": "1.0.1",
  "summary": "自己定义的播放器，附带3中类型的UI可选，基于FFmpeg与VLC,使用时可以自由选择",
  "description": "这是一个自己的项目，工具库集合，后续待补充",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/HDObject.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "PSL": "345455211@qq.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/HDObject.git",
    "tag": "1.0.1"
  },
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "ToolObject",
      "source_files": "HDObject/Classes/ToolObject/include/**/*.{h}",
      "vendored_libraries": "HDObject/Classes/ToolObject/lib/*.a",
      "frameworks": [
        "AVFoundation",
        "UIKit",
        "CoreTelephony",
        "Photos"
      ],
      "libraries": "iconv"
    },
    {
      "name": "HDFFMpeg",
      "source_files": "HDObject/Classes/HDFFMpeg/include/**/*.{h}",
      "vendored_libraries": "HDObject/Classes/HDFFMpeg/lib/*.a",
      "libraries": [
        "iconv",
        "z",
        "bz2"
      ],
      "frameworks": [
        "AVFoundation",
        "UIKit",
        "CoreTelephony",
        "Photos",
        "CoreMedia",
        "VideoToolbox",
        "AudioToolbox"
      ],
      "resources": "HDObject/Assets/HDFFMpeg/*.bundle",
      "dependencies": {
        "HDObject/ToolObject": [

        ],
        "MobileVLCKit": [
          "3.3.16.3"
        ],
        "Masonry": [
          "1.1.0"
        ],
        "ReactiveObjC": [
          "3.1.1"
        ],
        "FMDB": [
          "2.7.5"
        ],
        "UMCommon": [
          "7.3.5"
        ]
      }
    }
  ]
}
