{
  "name": "PepperMoveSDK",
  "version": "2.0.12",
  "authors": {
    "Pepper": "dev@pepper.me"
  },
  "summary": "iOS library for realtime video streaming from wifi cameras",
  "source": {
    "git": "https://git-codecommit.us-east-2.amazonaws.com/v1/repos/move-client-ios.git",
    "tag": "2.0.12",
    "branch": "feature/cocoapods"
  },
  "description": "Pepper iOS library for realtime video streaming from wifi cameras",
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "AVFoundation"
  ],
  "homepage": "https://pepper.me",
  "license": {
    "type": "PROPRIETARY",
    "file": "LICENSE.txt"
  },
  "source_files": "PepperMoveSDK/**/*.{m,h}",
  "public_header_files": [
    "PepperMoveSDK/CallController/PepperMove.h",
    "PepperMoveSDK/CallController/PepperMoveVideoSession.h"
  ],
  "private_header_files": [
    "PepperMoveSDK/*.h",
    "PepperMoveSDK/CallController/PepperMoveDelegateImpl.h",
    "PepperMoveSDK/CallController/RTCEAGLVideoViewDelegateImpl.h",
    "PepperMoveSDK/CallController/PepperVideoView.h",
    "PepperMoveSDK/CallController/*+Internal.h"
  ],
  "dependencies": {
    "PromisesObjC": [
      "~> 1.2"
    ],
    "JitsiWebRTC": [
      "~> 100.0"
    ],
    "SocketRocket": [
      "~> 0.5.1"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
