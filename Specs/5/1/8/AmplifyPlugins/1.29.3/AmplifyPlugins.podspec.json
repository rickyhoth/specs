        "AppSyncRealTimeClient": [
          "~> 3.0"
        ]
      }
    },
    {
      "name": "AWSCognitoAuthPlugin",
      "source_files": "AmplifyPlugins/Auth/AWSCognitoAuthPlugin/**/*.swift",
      "dependencies": {
        "AWSMobileClient": [
          "~> 2.30.1"
        ],
        "AWSAuthCore": [
          "~> 2.30.1"
        ],
        "AWSCognitoIdentityProvider": [
          "~> 2.30.1"
        ],
        "AWSCognitoIdentityProviderASF": [
          "~> 2.30.1"
        ]
