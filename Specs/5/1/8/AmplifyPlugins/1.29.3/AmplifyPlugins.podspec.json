    "tag": "1.29.3"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "dependencies": {
    "AWSPluginsCore": [
      "1.29.3"
    ],
    "AWSCore": [
      "~> 2.30.1"
    ]
  },
  "subspecs": [
