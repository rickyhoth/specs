{
  "name": "YMCallUIKit",
  "version": "0.1.3",
  "summary": "译码小二郞音视频呼叫界面库(YMCallUIKit)",
  "description": "基于WebRTC实现的单人音视频，多人音视频的即时通讯场景。译码小二郞在原有的基础上进行了优化。",
  "homepage": "https://gitee.com/yimaxiaoerlang",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yimaxiaoerlang": "yingxinjituan0532@163.com"
  },
  "source": {
    "git": "https://gitee.com/yimaxiaoerlang/ymcall-uikit-ios",
    "tag": "0.1.3"
  },
  "platforms": {
    "ios": "10.0"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true,
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "AudioToolbox",
    "AVFoundation",
    "VideoToolbox"
  ],
  "resources": [
    "YMCallUIKit/Assets/*.bundle"
  ],
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "vendored_frameworks": "YMCallKit.framework",
  "dependencies": {
    "YMCallLib": [
      "~> 0.1.3"
    ],
    "Masonry": [

    ]
  }
}
