{
  "name": "QNRTCKit",
  "version": "3.1.2",
  "summary": "Qiniu RTC SDK for iOS.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/QNRTC-iOS",
  "license": "Apache License, Version 2.0",
  "authors": {
    "pili": "pili-coresdk@qiniu.com"
  },
  "source": {
    "http": "https://sdk-release.qnsdk.com/QNRTCKit-iphoneos-v3.1.2.zip"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "CoreGraphics",
    "CFNetwork",
    "AudioToolbox",
    "CoreMedia",
    "VideoToolbox"
  ],
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": [
        "Pod/iphoneos/*.framework"
      ]
    },
    {
      "name": "ex-HappyDNS",
      "vendored_frameworks": [
        "Pod/iphoneos/QNRTCKit.framework"
      ]
    }
  ]
}
