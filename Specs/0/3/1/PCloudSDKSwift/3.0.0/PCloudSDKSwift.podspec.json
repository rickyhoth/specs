{
  "name": "PCloudSDKSwift",
  "version": "3.0.0",
  "summary": "Swift SDK for the pCloud API",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/pcloud-sdk-swift",
  "license": "MIT",
  "authors": "pCloud",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/pcloud-sdk-swift.git",
    "tag": "v3.0.0"
  },
  "swift_versions": "5",
  "platforms": {
    "osx": "10.11",
    "ios": "9.0"
  },
  "osx": {
    "frameworks": [
      "AppKit",
      "Webkit",
      "SystemConfiguration",
      "Foundation"
    ],
    "public_header_files": "PCloudSDKSwift/Source/**/*.h",
    "source_files": [
      "PCloudSDKSwift/Source/**/*.{swift,h}",
      "PCloudSDKSwift/SDK macOS/**/*.swift"
    ],
    "resource_bundles": {
      "PCloudSDKSwiftResources": "PCloudSDKSwift/SDK macOS/*.xib"
    }
  },
  "ios": {
    "frameworks": [
      "UIKit",
      "Webkit",
      "SystemConfiguration",
      "Foundation"
    ],
    "public_header_files": "PCloudSDKSwift/Source/**/*.h",
    "source_files": [
      "PCloudSDKSwift/Source/**/*.{swift,h}",
      "PCloudSDKSwift/SDK iOS/**/*.swift"
    ]
  },
  "requires_arc": true,
  "swift_version": "5"
}
