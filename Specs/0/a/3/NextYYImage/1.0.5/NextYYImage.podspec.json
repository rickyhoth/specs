{
  "name": "NextYYImage",
  "summary": "Image framework for iOS to display/encode/decode animated WebP, APNG, GIF, and more. YYImage based, but support Apple Silicon arm64 simulator.",
  "version": "1.0.5",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "pcjbird": "pcjbird@hotmail.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/NextYYImage.git",
    "tag": "1.0.5"
  },
  "social_media_url": "http://blog.ibireme.com",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/NextYYImage",
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "YYImage/*.{h,m}",
      "public_header_files": "YYImage/*.{h}",
      "libraries": "z",
      "frameworks": [
        "UIKit",
        "CoreFoundation",
        "QuartzCore",
        "AssetsLibrary",
        "ImageIO",
        "Accelerate",
        "MobileCoreServices"
      ]
    },
    {
      "name": "WebP",
      "dependencies": {
        "YYImage/Core": [

        ]
      },
      "ios": {
        "vendored_frameworks": "Vendor/WebP.xcframework"
      }
    },
    {
      "name": "libwebp",
      "dependencies": {
        "YYImage/Core": [

        ],
        "libwebp": [

        ]
      },
      "xcconfig": {
        "USER_HEADER_SEARCH_PATHS": "$(inherited) $(SRCROOT)/libwebp/src"
      }
    }
  ]
}
