{
  "name": "Brightroom",
  "version": "2.8.0",
  "summary": "A component-oriented image editor on top of CoreImage.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/Brightroom",
  "license": "MIT",
  "authors": "muukii",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/Brightroom.git",
    "tag": "2.8.0"
  },
  "swift_versions": "5.6",
  "module_name": "Brightroom",
  "requires_arc": true,
  "platforms": {
    "ios": "13.0"
  },
  "ios": {
    "frameworks": [
      "UIKit",
      "CoreImage"
    ],
    "dependencies": {
      "Verge/Store": [
        ">= 8.19.0"
      ]
    }
  },
  "subspecs": [
    {
      "name": "Engine",
      "source_files": "Sources/BrightroomEngine/**/*.swift"
    },
    {
      "name": "UI-Classic",
      "source_files": [
        "Sources/BrightroomUI/Shared/**/*.swift",
        "Sources/BrightroomUI/Built-in UI/ClassicImageEdit/**/*.swift"
      ],
      "dependencies": {
        "Brightroom/Engine": [

        ],
        "TransitionPatch": [

        ]
      },
      "resource_bundles": {
        "BrightroomUI": [
          "Sources/BrightroomUI/Media.xcassets"
        ]
      }
    },
    {
      "name": "UI-Crop",
      "source_files": [
        "Sources/BrightroomUI/Shared/**/*.swift",
        "Sources/BrightroomUI/Built-in UI/PhotosCrop/**/*.swift"
      ],
      "dependencies": {
        "Brightroom/Engine": [

        ]
      },
      "resource_bundles": {
        "BrightroomUI": [
          "Sources/BrightroomUI/Media.xcassets"
        ]
      }
    }
  ],
  "swift_version": "5.6"
}
