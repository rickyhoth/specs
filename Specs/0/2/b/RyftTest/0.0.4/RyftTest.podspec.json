{
  "name": "RyftTest",
  "version": "0.0.4",
  "summary": "Ryft SDK for iOS",
  "description": "Ryft for iOS allows you to accept in-app payments securely and safely using our customisable UI elements.",
  "homepage": "https://ryftpay.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ryft": "support@ryftpay.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/RyftTest.git",
    "tag": "v0.0.4",
    "submodules": true
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.1",
  "frameworks": "Foundation",
  "default_subspecs": [
    "RyftCard",
    "RyftCore"
  ],
  "pod_target_xcconfig": {
    "SWIFT_SUPPRESS_WARNINGS": "YES"
  },
  "subspecs": [
    {
      "name": "RyftCore",
      "source_files": "RyftCore/Sources/**/*.swift"
    },
    {
      "name": "RyftCard",
      "source_files": "RyftCard/Sources/**/*.swift"
    },
    {
      "name": "RyftUI",
      "source_files": "RyftUI/Sources/**/*.swift",
      "dependencies": {
        "RyftTest/RyftCore": [

        ],
        "RyftTest/RyftCard": [

        ]
      }
    }
  ],
  "swift_version": "5.1"
}
