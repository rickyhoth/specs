  "dependencies": {
    "AptoSDK": [
      "3.20.0"
    ],
    "AptoPCI": [
      "3.2.0"
    ],
    "SnapKit": [
      "~> 5.0"
    ],
    "Bond": [
      "~> 7.6"
    ],
    "GoogleKit": [
      "~> 0.3"
    ],
    "PhoneNumberKit": [
      "~> 3.3.3"
    ],
    "TTTAttributedLabel": [
      "~> 2.0"
    ],
    "TrustKit": [
      "~> 1.6.0"
    ],
    "Down": [
      "~> 0.8.0"
