{
  "name": "AptoUISDK",
  "version": "3.20.0",
  "summary": "The Apto UI platform iOS SDK.",
  "description": "Apto iOS UI SDK provides a UI flow that allows to easily integrate the platform in your app.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/apto-ui-sdk-ios.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Apto Payments": "mobile@aptopayments.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/apto-ui-sdk-ios.git",
    "tag": "3.20.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "module_name": "AptoUISDK",
  "source_files": [
    "Pod/Classes/ui/**/*.swift"
