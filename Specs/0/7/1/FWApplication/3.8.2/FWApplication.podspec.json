{
  "name": "FWApplication",
  "version": "3.8.2",
  "summary": "ios application framework",
  "homepage": "http://wuyong.site",
  "license": "MIT",
  "authors": {
    "Wu Yong": "admin@wuyong.site"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/FWApplication.git",
    "tag": "3.8.2"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "default_subspecs": [
    "FWApplication",
    "Compatible"
  ],
  "subspecs": [
    {
      "name": "FWApplication",
      "source_files": "FWApplication/Classes/FWApplication/**/*.{h,m}",
      "libraries": "sqlite3",
      "dependencies": {
        "FWFramework": [
          "~> 3.8.0"
        ]
      }
    },
    {
      "name": "Compatible",
      "source_files": "FWApplication/Classes/Compatible/**/*.swift",
      "dependencies": {
        "FWApplication/FWApplication": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited)"
      }
    },
    {
      "name": "SwiftUI",
      "weak_frameworks": [
        "SwiftUI",
        "Combine"
      ],
      "source_files": "FWApplication/Classes/SwiftUI/**/*.swift",
      "dependencies": {
        "FWApplication/Compatible": [

        ]
      }
    },
    {
      "name": "SDWebImage",
      "source_files": "FWApplication/Classes/SDWebImage/**/*.{h,m,swift}",
      "dependencies": {
        "SDWebImage": [

        ],
        "FWApplication/FWApplication": [

        ]
      }
    },
    {
      "name": "Lottie",
      "source_files": "FWApplication/Classes/Lottie/**/*.{h,m,swift}",
      "dependencies": {
        "lottie-ios": [

        ],
        "FWApplication/FWApplication": [

        ]
      }
    },
    {
      "name": "SQLCipher",
      "dependencies": {
        "SQLCipher": [

        ],
        "FWApplication/FWApplication": [

        ]
      },
      "xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DSQLITE_HAS_CODEC -DHAVE_USLEEP=1"
      }
    }
  ],
  "swift_version": "5.0"
}
