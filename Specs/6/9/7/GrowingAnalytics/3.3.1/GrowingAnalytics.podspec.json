    {
      "name": "DISABLE_IDFA",
      "dependencies": {
        "GrowingAnalytics/TrackerCore": [

        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "GROWING_ANALYSIS_DISABLE_IDFA=1"
      }
    },
    {
      "name": "ENABLE_ENCRYPTION",
      "dependencies": {
        "GrowingAnalytics/TrackerCore": [

        ]
      },
