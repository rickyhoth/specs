{
  "name": "PlayKit_IMA",
  "version": "1.13.1",
  "authors": {
    "Kaltura": "community@kaltura.com"
  },
  "license": {
    "type": "AGPLv3",
    "text": "AGPLv3"
  },
  "summary": "PlayKit IMA Plugin",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/playkit-ios-ima",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/playkit-ios-ima.git",
    "tag": "v1.13.1"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "12.0",
    "tvos": "12.0"
  },
  "dependencies": {
    "PlayKit": [
      "~> 3.22"
    ]
  },
  "ios": {
    "source_files": [
      "Sources/*.swift",
      "Sources/iOS/*.swift"
    ],
    "dependencies": {
      "GoogleAds-IMA-iOS-SDK": [
        "3.18.1"
      ]
    }
  },
  "tvos": {
    "source_files": [
      "Sources/*.swift",
      "Sources/tvOS/*.swift"
    ],
    "dependencies": {
      "GoogleAds-IMA-tvOS-SDK": [
        "4.6.1"
      ]
    }
  },
  "xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
