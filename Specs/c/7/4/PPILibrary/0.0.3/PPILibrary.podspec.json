  },
  "social_media_url": "https://bitbucket.devops.nonprod.empf.local/scm/react/ShenYj.github.io/wiki",
  "requires_arc": true,
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5"
  ],
  "cocoapods_version": ">= 1.10.0",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/PPILibrary.git",
    "tag": "0.0.3"
  },
  "summary": "基础工具扩展库.",
  "description": "个人开发的基础工具扩展库, 通过subspec增加了一些依赖库的扩展支持",
  "default_subspecs": "Core",
  "platforms": {
    "ios": "12.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Sources/Core/",
        "Sources/Core/Protocol/",
        "Sources/Core/Extension/"
      ],
      "frameworks": [
        "Foundation",
        "UIKit"
      ]
    },
    {
      "name": "Rx",
      "source_files": [
        "Sources/Rx/",
        "Sources/Rx/RxErrorTracker/",
        "Sources/Rx/RxActivityIndicator/"
      ],
      "dependencies": {
        "PPILibrary/Core": [

        ],
        "RxSwift": [
          "~> 6.0"
        ],
        "RxCocoa": [
          "~> 6.0"
        ],
        "RxSwiftExt": [
          "~> 6.0"
        ],
        "RxOptional": [
          "~> 5.0"
        ],
        "NSObject+Rx": [
          "~> 5.0"
        ],
        "RxGesture": [
          "~> 4.0"
        ],
        "RxDataSources": [
          "~> 5.0"
        ]
      }
    }
  ],
  "swift_version": "5.5"
}
