{
  "name": "WechatNoPaySwiftPod",
  "version": "1.9.2.2",
  "summary": "wechat without pay pod framework swift support",
  "swift_versions": "5",
  "description": "wechat sdk pod support for Swift",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/WechatSwiftPod",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "spec": "darkangel@gmail.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/WechatSwiftPod.git",
    "tag": "1.9.2.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source_files": [
    "WechatSwiftPod/*.h",
    "WechatSwiftPod/*.modulemap"
  ],
  "static_framework": true,
  "libraries": [
    "c++",
    "z"
  ],
  "frameworks": [
    "Security",
    "CoreGraphics",
    "WebKit"
  ],
  "vendored_libraries": "WechatSwiftPod/libWeChatSDK.a",
  "preserve_paths": "WechatSwiftPod/WechatSwiftPod.modulemap",
  "xcconfig": {
    "LD_RUNPATH_SEARCH_PATHS": "$(PODS_ROOT)/WechatSwiftPod"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "OTHER_LDFLAGS": "-ObjC -all_load",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5"
}
