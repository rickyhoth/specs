{
  "name": "BKDevMenu",
  "version": "1.0.0",
  "summary": "app端调试菜单入口",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/BKDevMenu",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Frank Lu": "arloou@gmail.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/BKDevMenu.git",
    "tag": "1.0.0"
  },
  "source_files": "BKDevMenu/src/*.{h,m}",
  "public_header_files": "BKDevMenu/src/*.{h}",
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "frameworks": "UIKit",
  "default_subspecs": [
    "privateClass",
    "sandbox",
    "console"
  ],
  "subspecs": [
    {
      "name": "privateClass",
      "source_files": "BKDevMenu/src/privateClass/*.{h,m}",
      "frameworks": "UIKit"
    },
    {
      "name": "sandbox",
      "source_files": "BKDevMenu/src/sandbox/*.{h,m}",
      "frameworks": "UIKit",
      "resources": "BKDevMenu/src/sandbox/LSBResources.bundle"
    },
    {
      "name": "console",
      "source_files": "BKDevMenu/src/console/*.{h,m}",
      "frameworks": "UIKit",
      "resources": "BKDevMenu/src/console/BKConsoleResources.bundle"
    }
  ]
}
