        "iProov": [
          "9.2.0"
        ]
      }
    },
    {
      "name": "NFC",
      "vendored_frameworks": "JumioNFC/Jumio.xcframework"
    },
    {
      "name": "NFCLiveness",
      "vendored_frameworks": "JumioNFC+Liveness/Jumio.xcframework",
