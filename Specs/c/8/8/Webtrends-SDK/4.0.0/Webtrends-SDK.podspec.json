{
  "name": "Webtrends-SDK",
  "version": "4.0.0",
  "summary": "The mobile SDK for Webtrends Analytics",
  "license": "COMMERCIAL",
  "homepage": "https://webtrends.com",
  "authors": {
    "webtrends": "support@webtrends.com"
  },
  "platforms": {
    "ios": "12.1"
  },
  "ios": {
    "frameworks": [
      "Foundation",
      "UIKit",
      "SystemConfiguration",
      "Security",
      "CoreTelephony",
      "WebKit"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/ios-sdk.git",
    "tag": "v4.0.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Headers",
        "Headers/**/*.h"
      ],
      "exclude_files": "Headers/Exclude",
      "resources": "SharedAssets/*",
      "libraries": [
        "z",
        "sqlite3"
      ],
      "vendored_libraries": "libWebtrendsSDK.a"
    }
  ]
}
