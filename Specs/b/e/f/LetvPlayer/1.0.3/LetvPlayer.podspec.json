{
  "name": "LetvPlayer",
  "version": "1.0.3",
  "summary": "乐视视频播放SDK.",
  "description": "乐视视频iOS播放SDK，主要提供通过vid获取p2p播放地址及CDN播放地址，提供p2p功能",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/LetvPlayer",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "letv": "liuzhibin@letv.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/LetvPlayer.git",
    "tag": "1.0.3"
  },
  "source_files": [
    "LetvPlayer/*.{h,m}",
    "LetvPlayer/**/*.{h,m}",
    "3rd/*.h"
  ],
  "public_header_files": [
    "LetvPlayer/LetvPlayer.h",
    "LetvPlayer/LetvPlayerManager.h",
    "LetvPlayer/LetvPlayerSdkController.h"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS[sdk=iphonesimulator*]": ""
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "frameworks": "Foundation",
  "libraries": "c++",
  "requires_arc": true,
  "vendored_libraries": "3rd/*.a",
  "xcconfig": {
    "USER_HEADER_SEARCH_PATHS": "3rd/*.h"
  },
  "dependencies": {
    "ReactiveObjC": [
      "~> 3.1.0"
    ],
    "AFNetworking": [
      "~> 4.0"
    ],
    "JSONModel": [

    ]
  }
}
