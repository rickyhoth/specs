        }
      },
      "tvos": {
        "frameworks": [
          "SystemConfiguration",
          "CoreGraphics"
        ],
        "libraries": [
          "z",
          "c++"
        ],
        "pod_target_xcconfig": {
          "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64",
