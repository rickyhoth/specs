{
  "name": "CCMediaConvert",
  "version": "1.0.2",
  "summary": "A framwork to convert media format",
  "description": "This is a component of Tailor which coding for file manager",
  "homepage": "https://gitee.com/Lucyfa/ccmedia-format-conver.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Lucyfa": "lccjust123@163.com"
  },
  "source": {
    "git": "https://gitee.com/Lucyfa/ccmedia-format-conver.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "CCMediaConvert/Classes/**/*",
  "dependencies": {
    "Masonry": [
      "1.1.0"
    ]
  },
  "vendored_frameworks": [
    "CCMediaConvert/Frameworks/**/*.framework"
  ],
  "frameworks": [
    "UIKit",
    "Photos",
    "ImageIO",
    "AVFoundation",
    "CoreGraphics",
    "MobileCoreServices",
    "Foundation",
    "WebKit"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true,
  "resource_bundles": {
    "CCMediaConvert": [
      "CCMediaConvert/Assets/**/*"
    ]
  }
}
