{
  "name": "SmallStrings",
  "version": "0.1.3",
  "summary": "A minifier for localized .strings files",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Emerge Tools",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/SmallStrings"
  },
  "homepage": "https://www.emergetools.com/",
  "platforms": {
    "ios": "11.0"
  },
  "source_files": [
    "Source/*.{m,h}"
  ],
  "preserve_paths": [
    "compress",
    "localize.rb",
    "localize.sh"
  ],
  "prepare_command": "clang -O3 compress.m -framework Foundation -lcompression -o compress",
  "libraries": "compression"
}
