{
  "name": "PermissionsKit",
  "version": "0.0.2",
  "summary": "Ask for a permissions on Swift. All permissions are available. Ability to get the state",
  "homepage": "https://gitlab.com/tedmob/ios/permissionskit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
