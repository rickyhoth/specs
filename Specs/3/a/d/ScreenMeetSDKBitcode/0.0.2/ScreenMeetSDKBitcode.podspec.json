{
  "name": "ScreenMeetSDKBitcode",
  "version": "0.0.2",
  "summary": "ScreenMeetSDK enables ScreenMeet's realtime platform in your app.",
  "description": "ScreenMeet provides a platform that allows you to build realtime solutions for your application.",
  "homepage": "https://bitbucket.org/projectorinc/ios-sdk-live-bitcode-test.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "ScreenMeet": "sdksupport@screenmeet.com"
  },
  "source": {
    "git": "https://bitbucket.org/projectorinc/ios-sdk-live-bitcode-test.git",
    "tag": "0.0.2"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "12.0"
  },
  "source_files": "ScreenMeetSDK/Classes/**/*",
  "vendored_frameworks": "ScreenMeetSDK/WebRTC.framework",
  "ios": {
    "xcconfig": {
      "HEADER_SEARCH_PATHS": "ScreenMeetSDK"
    }
  },
  "dependencies": {
    "Socket.IO-Client-Swift": [
      "~> 15.2.0"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "ENABLE_BITCODE": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
