{
  "name": "ShgardiMaps",
  "version": "2.1.0",
  "summary": "A short description of ShgardiMaps.",
  "description": "TODO: Add long description of the pod here.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/shgardi-iOS-maps",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Shgardi": "https://shgardi.app"
  },
  "source": {
    "git": "git@github.com:Shgardi/shgardi-iOS-maps",
    "branch": "master"
  },
  "default_subspecs": "Core",
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "platforms": {
    "ios": "13.0"
  },
  "static_framework": true,
  "subspecs": [
    {
      "name": "Core",
      "source_files": "ShgardiMaps/Classes/Core/**/*",
      "resources": [
        "ShgardiMaps/Assets/**/*"
      ],
      "frameworks": "UIKit",
      "dependencies": {
        "ShgardiFoundation": [

        ],
        "ShgardiUIBuilder": [

        ],
        "ShgardiUIFoundation": [

        ],
        "GooglePlaces": [
          "~>  7.2.0"
        ],
        "GoogleMaps": [
          "~>  7.2.0"
        ],
        "Google-Maps-iOS-Utils": [

        ]
      }
    },
    {
      "name": "Navigation",
      "source_files": "ShgardiMaps/Classes/Navigation/**/*",
      "ios": {
        "vendored_frameworks": "Frameworks/GoogleNavigation.framework"
      },
      "resources": [
        "Frameworks/GoogleNavigation.framework/Resources/*"
      ],
      "dependencies": {
        "ShgardiMaps/Core": [

        ]
      }
    },
    {
      "name": "RSDriver",
      "source_files": "ShgardiMaps/Classes/RSDriver/**/*",
      "ios": {
        "vendored_frameworks": "Frameworks/GoogleRidesharingDriver.framework"
      },
      "resources": [
        "Frameworks/GoogleRidesharingDriver.framework/Resources/*"
      ],
      "dependencies": {
        "ShgardiMaps/Navigation": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
