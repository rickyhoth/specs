{
  "name": "MLCardForm",
  "version": "0.14.0",
  "summary": "MLCardForm for iOS",
  "homepage": "https://www.mercadolibre.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "PX Team - Juan Sanzone - Eric Ertl - Esteban Boffa",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/card-form-ios",
    "tag": "0.14.0"
  },
  "swift_versions": "5.6",
  "platforms": {
    "ios": "13.0"
  },
  "requires_arc": true,
  "default_subspecs": "Default",
  "static_framework": true,
  "dependencies": {
    "MLUI": [
      "~> 5.0"
    ],
    "AndesUI": [
      "~> 3.0"
    ],
    "MLCardDrawer": [
      "~> 1.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "requires_app_host": false,
      "source_files": "Tests/**/*.{swift}",
      "frameworks": "XCTest"
    }
  ],
  "subspecs": [
    {
      "name": "Default",
      "source_files": [
        "Source/**/**/**/*.{h,m,swift}"
      ],
      "resources": [
        "Source/UI/**/*.xib"
      ],
      "resource_bundles": {
        "MLCardFormResources": [
          "Source/Resources/*.xcassets",
          "Source/Translations/**/**.{strings}"
        ]
      }
    }
  ],
  "swift_version": "5.6"
}
