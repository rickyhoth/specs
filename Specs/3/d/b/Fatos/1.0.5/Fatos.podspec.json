{
  "name": "Fatos",
  "version": "1.0.5",
  "summary": "FATOS Mogos SDK for internal distribution via gitlab.",
  "description": "Software Development Kit to display 3d map and provide navigation functionalities.",
  "homepage": "https://git.fatos.biz/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "FATOS": "dev@fatoscorp.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/mogos-fatos-ios-sdk.git",
    "tag": "1.0.5"
  },
  "resources": "ResFatos.bundle",
  "swift_versions": "5.0",
  "platforms": {
    "ios": "13.0"
  },
  "source_files": [
    "include/*.h"
  ],
  "preserve_paths": "module/module.modulemap",
  "module_map": "module/module.modulemap",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "OTHER_LDFLAGS": [
      "-ObjC",
      "-lc++",
      ""
    ],
    "DEFINES_MODULE": "YES",
    "OTHER_SWIFT_FLAGS": "",
    "HEADER_SEARCH_PATHS": " /Fatos/module",
    "SWIFT_INCLUDE_PATHS": "/include"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "frameworks": [
    "GLKit",
    "OpenGLES",
    "QuartzCore",
    "UIKit",
    "Foundation",
    "CoreGraphics",
    "CoreAudio",
    "WebKit",
    "CoreTelephony",
    "CoreLocation",
    "AudioToolBox",
    "AVFoundation"
  ],
  "vendored_libraries": "libFatos.a",
  "swift_version": "5.0"
}
