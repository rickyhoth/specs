{
  "name": "EMTranslate",
  "version": "0.1.0",
  "license": {
    "type": "Copyright",
    "text": "Hyphenate Inc. 2017"
  },
  "summary": "A translate framework on HyphenateChat IM SDK",
  "description": "EMTranslate is a translate tool which depend on HyphenateChat ^3.8.9 SDK.You can translate text to other language which you want.",
  "homepage": "http://www.easemob.com/",
  "authors": {
    "Hyphenate Inc.": "sdk@easemob.com"
  },
  "source": {
    "http": "https://downloadsdk.easemob.com/downloads/EMTranslate0_1_0.zip"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "preserve_paths": "*.framework",
  "vendored_frameworks": "*.framework",
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "arm64 armv7 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
