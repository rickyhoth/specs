{
  "name": "lua-iosx",
  "version": "5.4.5.0",
  "summary": "LUA XCFramework for macOS and iOS, including both arm64 and x86_64 builds for macOS, Mac Catalyst and Simulator.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/lua-iosx",
  "license": "MIT",
  "authors": {
    "Alexander Pototskiy": "alex.a.potocki@gmail.com"
  },
  "social_media_url": "https://www.linkedin.com/in/alexander-pototskiy",
  "platforms": {
    "ios": "13.4",
    "osx": "11.0"
  },
  "osx": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "ios": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "static_framework": true,
  "prepare_command": "sh scripts/build.sh",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/lua-iosx.git",
    "tag": "5.4.5.0",
    "submodules": "true"
  },
  "source_files": "frameworks/Headers/*.{h}",
  "header_mappings_dir": "frameworks/Headers",
  "public_header_files": "frameworks/Headers/*.{h,hpp}",
  "vendored_frameworks": "frameworks/lua.xcframework"
}
