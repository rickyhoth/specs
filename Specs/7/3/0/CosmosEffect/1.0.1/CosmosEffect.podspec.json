{
  "name": "CosmosEffect",
  "version": "1.0.1",
  "summary": "beauty kit",
  "description": "beauty kit libraries",
  "homepage": "http://git.live.e.weibo.com/YXThirdPartyGroup/CosmosEffect",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "SpielbergGao": "spielberggao@gmail.com"
  },
  "source": {
    "git": "ssh://git@git.live.e.weibo.com:2223/YXThirdPartyGroup/CosmosEffect.git",
    "tag": "3.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "$(inherited) NO",
    "OTHER_LDFLAGS": "$(inherited) -ObjC",
    "MTL_LANGUAGE_REVISION": "Metal12",
    "GCC_PREPROCESSOR_DEFINITIONS": "STICKER",
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "$(inherited) NO"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "vendored_frameworks": "Frameworks/**/*.framework",
  "resources": [
    "Frameworks/**/*.bundle",
    "*.bundle"
  ],
  "dependencies": {
  },
  "frameworks": [
    "Accelerate",
    "CoreImage",
    "AssetsLibrary",
    "OpenAL",
    "CoreML",
    "CoreMotion",
    "Metal"
  ],
  "libraries": [
    "c++",
    "iconv",
    "z"
  ]
}
