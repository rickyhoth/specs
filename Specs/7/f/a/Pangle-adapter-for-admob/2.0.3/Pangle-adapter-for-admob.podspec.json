{
  "name": "Pangle-adapter-for-admob",
  "version": "2.0.3",
  "summary": "Integrating Pangle with Admob Mediation",
  "description": "For publishers who want to use the Google Mobile Ads SDK to load and display ads from Pangle via mediation.",
  "license": {
    "type": "MIT",
    "file": "AdmobAdapter/LICENSE"
  },
  "authors": {
    "zengyang": "zywork@bytedance.com"
  },
  "homepage": "https://www.pangle.cn",
  "source": {
    "http": "https://sf16-fe-tos-sg.i18n-pglstatp.com/obj/pangle-sdk-static-va/Adapter/Admob/2.0.3/AdmobAdapter.zip"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 arm64e armv7 armv7s",
    "EXCLUDED_ARCHS[sdk=iphoneos*]": "i386 x86_64"
  },
  "platforms": {
    "ios": "10.0"
  },
  "static_framework": true,
  "source_files": "AdmobAdapter/*.{h,m}",
  "dependencies": {
    "Ads-Global": [
      ">= 4.6.2.2"
    ],
    "Google-Mobile-Ads-SDK": [
      ">= 9.0.0"
    ]
  }
}
