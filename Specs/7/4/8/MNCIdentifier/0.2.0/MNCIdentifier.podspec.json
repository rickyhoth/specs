{
  "name": "MNCIdentifier",
  "version": "0.2.0",
  "summary": "iOS SDK platforms for KYC verification services.",
  "description": "This KYC SDK involves steps to establish customer identity thorough liveness detection.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/mnc-identifiersdk-ios",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Ihksan Sukmawan": "ihksan.sukmawan@mncgroup.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/mnc-identifiersdk-ios.git",
    "tag": "0.2.0"
  },
  "frameworks": "UIKit",
  "default_subspecs": "OCR",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "Face",
      "vendored_frameworks": "Face.framework",
      "dependencies": {
        "GoogleMLKit/FaceDetection": [
          "2.3.0"
        ]
      }
    },
    {
      "name": "OCR",
      "vendored_frameworks": "OCR.framework",
      "dependencies": {
        "GoogleMLKit/TextRecognition": [
          "2.3.0"
        ],
        "GoogleMLKit/ObjectDetection": [
          "2.3.0"
        ]
      }
    }
  ]
}
