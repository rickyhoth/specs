{
  "name": "EZXAudio",
  "version": "0.0.3",
  "summary": "This a audio waveform plugin",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/EZXAudio",
  "license": "MIT ()",
  "authors": {
    "J999999": "1010710307@qq.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/EZXAudio.git",
    "tag": "0.0.3"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.8"
  },
  "exclude_files": [
    "EZXAudio/TPCircularBuffer.{h,c}",
    "EZXAudio/EZAudioiOS.h",
    "EZXAudio/EZAudioOSX.h"
  ],
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "GLKit",
      "Accelerate"
    ]
  },
  "osx": {
    "frameworks": [
      "AudioToolbox",
      "AudioUnit",
      "CoreAudio",
      "QuartzCore",
      "OpenGL",
      "GLKit",
      "Accelerate"
    ]
  },
  "requires_arc": true,
  "default_subspecs": "Full",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "EZXAudio/*.{h,m,c}"
    },
    {
      "name": "Full",
      "dependencies": {
        "TPCircularBuffer": [
          "1.1"
        ],
        "EZXAudio/Core": [

        ]
      }
    }
  ]
}
