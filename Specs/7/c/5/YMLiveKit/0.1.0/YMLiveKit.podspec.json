{
  "name": "YMLiveKit",
  "version": "0.1.0",
  "summary": "译码小二郎直播推流框架 YMLiveKit.",
  "description": "YMLiveKit 是译码小二郎自研的直播推流框架",
  "homepage": "https://gitee.com/yimaxiaoerlang",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Reyzhang2010": "zhanglei5415@163.com"
  },
  "source": {
    "git": "https://gitee.com/yimaxiaoerlang/ymlivekit-ios",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true,
  "requires_arc": true,
  "vendored_frameworks": "YMLiveKit.framework",
  "frameworks": [
    "UIKit",
    "Foundation",
    "AVFoundation",
    "OpenGLES",
    "VideoToolbox",
    "AudioToolbox"
  ],
  "libraries": [
    "c++",
    "bz2",
    "z",
    "iconv"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-Objc -lbz2 -lc++ -lsqlite3 -lz"
  },
  "dependencies": {
    "GPUImage": [
      "0.1.7"
    ],
    "Masonry": [

    ],
    "MBProgressHUD": [

    ],
    "SDWebImage": [

    ],
    "MJExtension": [

    ],
    "AFNetworking": [
      "4.0.1"
    ]
  }
}
