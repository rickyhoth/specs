{
  "name": "livemap-ios-sdk",
  "version": "2.1.0",
  "summary": "The Wemap IOS SDK.",
  "description": "The Wemap IOS SDK is a library that will handle communication with the Wemap application.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/livemap-ios-sdk",
  "license": "MIT",
  "authors": {
    "Thibault Capelli": "thibault.capelli@getwemap.com"
  },
  "source": {
    "git": "git@github.com:wemap/livemap-ios-sdk.git",
    "tag": "2.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": [
    "livemap-ios-sdk/**/*.{swift}",
    "CustomARView.{swift}",
    "Libraries/**/*.{modulemap,swift}"
  ],
  "resources": [
    "**/*.{xib, png, jpeg, jpg}"
  ],
  "xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Libraries",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "frameworks": [
    "CoreBluetooth",
    "CoreLocation",
    "CoreMotion",
    "SystemConfiguration"
  ],
  "static_framework": true,
  "libraries": [
    "c++",
    "z",
    "NAOSDK"
  ],
  "requires_arc": true,
  "dependencies": {
    "NAOSDK": [

    ]
  },
  "swift_versions": [
    "5"
  ],
  "swift_version": "5"
}
