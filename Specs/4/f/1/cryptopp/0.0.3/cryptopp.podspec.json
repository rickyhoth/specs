{
  "name": "cryptopp",
  "version": "0.0.3",
  "summary": "c++ cryptopp framework for ios",
  "description": "This pod serves as just includer of c++ cryptopp library, but it doesn't contain any swift or obj-c code.",
  "homepage": "https://gitlab.com/killalad/cryptopp-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "killalad": "martin.georgiu@gmail.com"
  },
  "source": {
    "git": "https://gitlab.com/killalad/cryptopp-ios",
    "tag": "0.0.3"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "4.0",
  "preserve_paths": "cryptopp.framework",
  "vendored_frameworks": "cryptopp.framework",
  "libraries": "c++",
  "xcconfig": {
    "OTHER_LDFLAGS": "-framework cryptopp"
  },
  "source_files": "cryptopp.framework/Headers/**/*{.h,.hpp}",
  "public_header_files": "cryptopp.framework/Headers/**/*{.h,.hpp}",
  "header_dir": "cryptopp",
  "header_mappings_dir": "cryptopp.framework/Headers/",
  "swift_version": "4.0"
}
