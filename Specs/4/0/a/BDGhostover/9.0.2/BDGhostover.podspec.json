{
  "name": "BDGhostover",
  "version": "9.0.2",
  "summary": "An SDK that allows to load and display Ghostover ads",
  "homepage": "https://docs.ghostover.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Bitdrome Srl",
  "source": {
    "http": "https://repo.ghostover.com/ios/cocoapods/BDGhostover902.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "10.0"
  },
  "ios": {
    "vendored_frameworks": "BDGhostover.xcframework"
  },
  "frameworks": [
    "SpriteKit",
    "SafariServices",
    "UIKit",
    "AVFoundation",
    "AdSupport",
    "QuartzCore"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS[sdk=iphonesimulator*]": "",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2",
    "OTHER_LDFLAGS": "-ObjC"
  }
}
