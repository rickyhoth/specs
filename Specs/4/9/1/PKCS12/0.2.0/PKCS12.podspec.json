{
  "name": "PKCS12",
  "version": "0.2.0",
  "summary": "PKCS12 Serialization Tools for ObjC.",
  "description": "Serialize *.p12 file to X509 certficate object.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/reactPKCS12",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Magic-Unique": "516563564@qq.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/reactPKCS12.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "PKCS12/Core/**/*",
      "public_header_files": "PKCS12/Core/Public/*.h",
      "dependencies": {
        "OpenSSL-Universal": [
          "~> 1.0.2.0"
        ]
      }
    },
    {
      "name": "OCSP",
      "source_files": "PKCS12/OCSP/**/*",
      "public_header_files": "PKCS12/OCSP/Public/*.h",
      "libraries": "c++",
      "dependencies": {
        "OpenSSL-Universal": [
          "~> 1.0.2.0"
        ],
        "PKCS12/Core": [

        ]
      }
    }
  ]
}
