{
  "name": "TapstreamIOS",
  "version": "4.0.0",
  "summary": "Tapstream marketing analytics SDK for iOS.",
  "homepage": "https://tapstream.com/",
  "license": "MIT",
  "authors": {
    "Tapstream": "support@tapstream.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/tapstream-sdk-ios.git",
    "tag": "v4.0.0-ios"
  },
  "platforms": {
    "ios": "11.0"
  },
  "ios": {
    "frameworks": [
      "Foundation",
      "UIKit",
      "WebKit"
    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "tapstream-sdk-ios",
        "tapstream-sdk-ios/ios-only"
      ]
    },
    {
      "name": "InAppLanders",
      "source_files": [
        "tapstream-sdk-ios",
        "tapstream-sdk-ios/ios-only",
        "tapstream-sdk-ios/in-app-landers"
      ],
      "resources": "tapstream-sdk-ios/in-app-landers/*.xib"
    },
    {
      "name": "WordOfMouth",
      "source_files": [
        "tapstream-sdk-ios",
        "tapstream-sdk-ios/ios-only",
        "tapstream-sdk-ios/word-of-mouth"
      ],
      "resources": "tapstream-sdk-ios/word-of-mouth/*.xib"
    }
  ]
}
