    "Filip Gulan": "filip.gulan@infinum.hr"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/Japx.git",
    "tag": "4.0.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "10.0",
    "osx": "10.12"
  },
  "swift_versions": "5.0",
