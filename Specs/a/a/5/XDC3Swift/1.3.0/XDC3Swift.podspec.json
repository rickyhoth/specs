{
  "name": "XDC3Swift",
  "version": "1.3.0",
  "summary": "XinFin API for swift",
  "description": "XinFin Swift API with support for smart contracts and XRC20",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/XDC3Swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "XinFin": "XFLW@xinfin.org"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/XDC3Swift.git",
    "tag": "1.3.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "source_files": [
    "Classes/src/**/*.swift",
    "Classes/lib/**/*.{c,h}",
    "Classes/src/XRC20/*.swift",
    "Classes/src/XRC721/*.swift",
    "Classes/src/Account/*.swift",
    "Classes/src/Client/*.swift",
    "Classes/src/Contract/*.swift",
    "Classes/src/Extensions/*.swift",
    "Classes/src/XDCUtils/*.swift"
  ],
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(PODS_TARGET_SRCROOT)/Classes/lib/**",
    "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(PODS_TARGET_SRCROOT)/Classes/lib/**"
  },
  "preserve_paths": "Classes/lib/**/module.map",
  "static_framework": true,
  "dependencies": {
    "GenericJSON": [
      "~> 2.0.0"
    ],
    "BigInt": [
      "~> 5.2.0"
    ],
    "secp256k1.swift": [
      "~> 0.1.0"
    ],
    "HDWalletKit": [
      "~> 0.3.6"
    ]
  },
  "swift_version": "5.0"
}
