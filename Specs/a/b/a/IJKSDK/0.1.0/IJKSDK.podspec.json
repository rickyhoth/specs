  },
  "frameworks": [
    "AudioToolbox",
    "AVFoundation",
    "CoreGraphics",
    "CoreMedia",
    "CoreVideo",
    "MediaPlayer",
    "MobileCoreServices",
    "OpenGLES",
    "QuartzCore",
    "UIKit",
    "VideoToolbox"
  ],
  "libraries": [
    "bz2",
    "c++",
    "z"
  ]
