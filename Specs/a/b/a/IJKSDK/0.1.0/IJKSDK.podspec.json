{
  "name": "IJKSDK",
  "version": "0.1.0",
  "summary": "IJKMediaPlayer",
  "description": "修改后的IJKMediaPlayer",
  "homepage": "https://fijkplayer.befovy.com/docs/zh/replace-ijkplayer.html",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "seedmorn_gf": "yangfeng@liuliangwan.com"
  },
  "source": {
    "git": "https://git.code.tencent.com/IJKSDK/IJKSDK.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "static_framework": true,
  "source_files": "IJKSDK/Classes/*.framework/Headers/**/*.h",
