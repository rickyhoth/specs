{
  "name": "SpockSwift",
  "version": "0.0.2",
  "summary": "Libreria para agulizar el desarrollo de pruebas",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/experimental-test-tool-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": "Francisco Javier Saldivar Rubio",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/experimental-test-tool-swift.git",
    "tag": "0.0.2"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "default_subspecs": [
    "SpockDummy",
    "SpockMock"
  ],
  "subspecs": [
    {
      "name": "SpockDummy",
      "source_files": "Sources/SpockDummy/source/**/*",
      "resource_bundles": {
        "SpockDummy": [
          "Sources/SpockDummy/assets/**/*"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "source_files": "Tests/SpockDummy/**/*"
        }
      ]
    },
    {
      "name": "SpockMock",
      "source_files": "Sources/SpockMock/source/**/*",
      "resource_bundles": {
        "SpockSpy": [
          "Sources/SpockMock/assets/**/*"
        ]
      },
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "dependencies": {
            "SpockSwift/SpockDummy": [

            ]
          },
          "source_files": "Tests/SpockMock/**/*"
        }
      ]
    }
  ],
  "swift_version": "5.0"
}
