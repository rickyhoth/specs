{
  "name": "TiltUpTest",
  "version": "3.0.0",
  "summary": "Official Clutter SDK in Swift to access core iOS test helpers.",
  "description": "TiltUpTest is a framework that provides utilities and mocks that are useful for\nwriting unit tests for components that are written using TiltUp.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/TiltUp",
  "authors": {
    "Clutter": "tech@clutter.com"
  },
  "license": "MIT",
  "platforms": {
    "ios": "13.3"
  },
  "swift_versions": [
    "5.1"
  ],
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "frameworks": "XCTest",
  "dependencies": {
    "TiltUp": [
      "= 3.0.0"
    ]
  },
  "source": {
    "git": "git@github.com:clutter/TiltUp.git",
    "tag": "3.0.0"
  },
  "source_files": "Sources/TiltUpTest/**/*",
  "swift_version": "5.1"
}
