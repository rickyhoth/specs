{
  "name": "HiCocoa",
  "version": "1.0.5",
  "summary": "iOS App Framework.",
  "description": "iOS App Framework using Swift.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/HiCocoa",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "YangJianxiang": "tospery@gmail.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/HiCocoa.git",
    "tag": "1.0.5"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "11.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreGraphics"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "HiCocoa/Core/**/*",
      "dependencies": {
        "QMUIKit/QMUICore": [
          "4.4.3"
        ],
        "FCUUID": [
          "1.3.1"
        ],
        "SwiftyBeaver": [
          "1.9.5"
        ],
        "ObjectMapper-JX": [
          "4.2.0-v1"
        ],
        "SwifterSwift": [
          "5.2.0"
        ]
      }
    },
    {
      "name": "Network",
      "source_files": "HiCocoa/Network/**/*",
      "dependencies": {
        "HiCocoa/Core": [

        ],
        "RxRelay": [
          "6.2.0"
        ],
        "Moya/RxSwift": [
          "15.0.0"
        ]
      }
    },
    {
      "name": "Router",
      "source_files": "HiCocoa/Router/**/*",
      "dependencies": {
        "HiCocoa/Core": [

        ],
        "RxSwift": [
          "~> 6.0"
        ],
        "RxCocoa": [
          "~> 6.0"
        ],
        "URLNavigator": [
          "2.3.0"
        ]
      }
    },
    {
      "name": "Cache",
      "source_files": "HiCocoa/Cache/**/*",
      "dependencies": {
        "HiCocoa/Core": [

        ],
        "Cache": [
          "6.0.0"
        ]
      }
    },
    {
      "name": "Theme",
      "source_files": "HiCocoa/Theme/**/*",
      "dependencies": {
        "HiCocoa/Core": [

        ],
        "RxTheme": [
          "6.0.0"
        ]
      }
    },
    {
      "name": "Resources",
      "resource_bundles": {
        "Resources": [
          "HiCocoa/Resources/*.*"
        ]
      }
    },
    {
      "name": "Components",
      "subspecs": [
        {
          "name": "JSBridge",
          "source_files": "HiCocoa/Components/JSBridge/**/*",
          "frameworks": "WebKit"
        }
      ]
    },
    {
      "name": "Frame",
      "source_files": "HiCocoa/Frame/**/*",
      "dependencies": {
        "HiCocoa/Core": [

        ],
        "HiCocoa/Cache": [

        ],
        "HiCocoa/Theme": [

        ],
        "HiCocoa/Router": [

        ],
        "HiCocoa/Network": [

        ],
        "HiCocoa/Resources": [

        ],
        "HiCocoa/Components/JSBridge": [

        ],
        "RxDataSources": [
          "5.0.0"
        ],
        "NSObject+Rx": [
          "5.2.2"
        ],
        "ReactorKit": [
          "3.2.0"
        ],
        "BonMot": [
          "6.0.0"
        ],
        "Kingfisher": [
          "6.3.1"
        ],
        "DZNEmptyDataSet": [
          "1.8.1"
        ],
        "MJRefresh": [
          "3.7.5"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
