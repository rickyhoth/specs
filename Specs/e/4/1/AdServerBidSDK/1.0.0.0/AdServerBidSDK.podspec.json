        "AdSupport",
        "CoreLocation",
        "QuartzCore",
        "SystemConfiguration",
        "CoreTelephony",
        "Security",
        "StoreKit",
        "AVFoundation",
        "WebKit"
      ],
      "libraries": [
        "xml2",
        "z"
      ]
    },
    {
      "name": "KS",
      "dependencies": {
        "AdServerBidSDK/Core": [

        ],
        "AdServerBidSDK/Adspot": [

        ],
        "KSAdSDK": [
          "3.3.36"
        ]
