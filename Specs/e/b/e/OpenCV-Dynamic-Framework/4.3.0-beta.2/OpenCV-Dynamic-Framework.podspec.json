{
  "name": "OpenCV-Dynamic-Framework",
  "version": "4.3.0-beta.2",
  "summary": "OpenCV dynamic framework",
  "description": "From https://bitbucket.devops.nonprod.empf.local/scm/react/opencv",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/OpenCV-Dynamic-Framework.git",
  "license": {
    "type": "3-clause BSD",
    "file": "LICENSE"
  },
  "authors": "https://opencv.org/",
  "platforms": {
    "ios": "11.1"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/OpenCV-Dynamic-Framework.git",
    "tag": "4.3.0-beta.2"
  },
  "prepare_command": "./extract.sh",
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source_files": "opencv2.framework/Headers/**/*{.h,.hpp}",
  "preserve_paths": "opencv2.framework",
  "vendored_frameworks": "opencv2.framework",
  "requires_arc": false,
  "ios": {
    "frameworks": [
      "AssetsLibrary",
      "AVFoundation",
      "CoreGraphics",
      "CoreMedia",
      "CoreVideo",
      "Foundation",
      "QuartzCore",
      "UIKit"
    ]
  },
  "libraries": "c++"
}
