{
  "name": "SwiftNIOSSL",
  "version": "2.19.0",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE.txt"
  },
  "summary": "TLS Support for SwiftNIO, based on BoringSSL.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/swift-nio-ssl",
  "authors": "Apple Inc.",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/swift-nio-ssl.git",
    "tag": "2.19.0"
  },
  "documentation_url": "https://apple.github.io/swift-nio-ssl/",
  "module_name": "NIOSSL",
  "swift_versions": "5.4",
  "cocoapods_version": ">=1.6.0",
  "platforms": {
    "ios": "10.0",
    "osx": "10.12",
    "tvos": "10.0",
    "watchos": "6.0"
  },
  "source_files": "Sources/NIOSSL/**/*.{swift,c,h,cc}",
  "ios": {
    "source_files": "Sources/NIOSSL/**/*.S"
  },
  "osx": {
    "source_files": "Sources/NIOSSL/**/*.S"
  },
  "tvos": {
    "source_files": "Sources/NIOSSL/**/*.S"
  },
  "watchos": {
    "pod_target_xcconfig": {
      "GCC_PREPROCESSOR_DEFINITIONS": "OPENSSL_NO_ASM=1",
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) OPENSSL_NO_ASM"
    }
  },
  "dependencies": {
    "CNIOBoringSSLShims": [
      "2.19.0"
    ],
    "CNIODarwin": [
      ">= 2.32.0",
      "< 3"
    ],
    "SwiftNIOTLS": [
      ">= 2.32.0",
      "< 3"
    ],
    "SwiftNIOPosix": [
      ">= 2.32.0",
      "< 3"
    ],
    "CNIOAtomics": [
      ">= 2.32.0",
      "< 3"
    ],
    "SwiftNIOEmbedded": [
      ">= 2.32.0",
      "< 3"
    ],
    "CNIOLinux": [
      ">= 2.32.0",
      "< 3"
    ],
    "CNIOWindows": [
      ">= 2.32.0",
      "< 3"
    ],
    "SwiftNIOCore": [
      ">= 2.32.0",
      "< 3"
    ],
    "_NIODataStructures": [
      ">= 2.32.0",
      "< 3"
    ],
    "CNIOBoringSSL": [
      "2.19.0"
    ],
    "SwiftNIO": [
      ">= 2.32.0",
      "< 3"
    ],
    "SwiftNIOConcurrencyHelpers": [
      ">= 2.32.0",
      "< 3"
    ]
  },
  "xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xcc -Wno-error=non-modular-include-in-framework-module"
  },
  "swift_version": "5.4"
}
