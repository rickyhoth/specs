{
  "name": "AzureIoTuMqtt",
  "version": "1.8.0-beta-1",
  "summary": "AzureIoTuMqtt library for CocoaPods.",
  "description": "This is a CocoaPods release of the Azure C uMQTT library.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/azure-iot-sdk-c/blob/master/doc/sdk_cocoapods.md",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Microsoft": ""
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/azure-umqtt-c.git",
    "commit": "ef6202575a44f1c3e339e67da8607ef5ed0c9f6a"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.10"
  },
  "preserve_paths": [
    "inc/*",
    "inc/**/*"
  ],
  "source_files": [
    "inc/**/*.h",
    "src/*.c"
  ],
  "header_mappings_dir": "inc/",
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "USE_HEADERMAP": "NO",
    "USER_HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/AzureIoTUtility/inc/\" \"${PODS_ROOT}/AzureIoTuMqtt/inc/\"",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/AzureIoTUtility/inc/\" \"${PODS_ROOT}/AzureIoTuMqtt/inc/\"",
    "ALWAYS_SEARCH_USER_PATHS": "NO"
  },
  "dependencies": {
    "AzureMacroUtils": [
      "1.3.9-beta-1"
    ],
    "AzureuMockC": [
      "1.3.9-beta-1"
    ],
    "AzureIoTUtility": [
      "1.8.0-beta-1"
    ]
  }
}
