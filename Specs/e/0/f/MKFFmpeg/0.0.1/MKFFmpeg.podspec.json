{
  "name": "MKFFmpeg",
  "version": "0.0.1",
  "summary": "FFmpeg static libraries compiled for iOS",
  "homepage": "https://gitee.com/duanshaoxiong/FFmpeg",
  "license": {
    "type": "LGPL",
    "file": "LICENSE"
  },
  "authors": {
    "Chris Ballinger": "chris@chatsecure.org"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://gitee.com/duanshaoxiong/FFmpeg.git",
    "tag": "0.0.1",
    "submodules": true
  },
  "swift_versions": "5.0",
  "source_files": [
    "ffmpeg-ios-static-libs/include/**/*.h"
  ],
  "public_header_files": [
    "ffmpeg-ios-static-libs/include/**/*.h"
  ],
  "header_mappings_dir": "ffmpeg-ios-static-libs/include",
  "vendored_libraries": [
    "ffmpeg-ios-static-libs/lib/*.a"
  ],
  "libraries": [
    "avcodec",
    "avfilter",
    "avformat",
    "avutil",
    "swresample",
    "swscale",
    "iconv",
    "z",
    "bz2"
  ],
  "frameworks": [
    "CoreMedia",
    "VideoToolBox",
    "AudioToolBox"
  ],
  "swift_version": "5.0"
}
