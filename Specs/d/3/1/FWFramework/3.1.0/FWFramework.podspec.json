{
  "name": "FWFramework",
  "version": "3.1.0",
  "summary": "ios develop framework",
  "homepage": "http://wuyong.site",
  "license": "MIT",
  "authors": {
    "Wu Yong": "admin@wuyong.site"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/FWFramework.git",
    "tag": "3.1.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "default_subspecs": [
    "FWFramework",
    "Compatible"
  ],
  "subspecs": [
    {
      "name": "FWFramework",
      "source_files": "FWFramework/Classes/FWFramework/**/*.{h,m}"
    },
    {
      "name": "Compatible",
      "source_files": "FWFramework/Classes/Compatible/**/*.swift",
      "dependencies": {
        "FWFramework/FWFramework": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited)"
      }
    },
    {
      "name": "Contacts",
      "dependencies": {
        "FWFramework/Compatible": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "FWMacroContacts"
      }
    },
    {
      "name": "Microphone",
      "dependencies": {
        "FWFramework/Compatible": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "FWMacroMicrophone"
      }
    },
    {
      "name": "Calendar",
      "dependencies": {
        "FWFramework/Compatible": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "FWMacroCalendar"
      }
    },
    {
      "name": "AppleMusic",
      "dependencies": {
        "FWFramework/Compatible": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "FWMacroAppleMusic"
      }
    },
    {
      "name": "Tracking",
      "dependencies": {
        "FWFramework/Compatible": [

        ]
      },
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "FWMacroTracking",
        "GCC_PREPROCESSOR_DEFINITIONS": "FWMacroTracking=1"
      }
    }
  ],
  "swift_version": "5.0"
}
