      "source_files": "kanzhunLib/kzmqttsdk/KZMqttSDK/MqttSDK/*.{h,m}"
    },
    {
      "name": "KZMenuAssistor",
      "source_files": "kanzhunLib/kzmenuassistor/KZMenuAssistor/KZMenuAssistor/**/*.{h,m}",
