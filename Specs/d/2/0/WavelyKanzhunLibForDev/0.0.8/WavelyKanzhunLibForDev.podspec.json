    {
      "name": "KZModelCast",
      "source_files": "kanzhunLib/kzmodelcast/KZModelCast/ModelCast/*.{h,m}"
    },
    {
      "name": "KZURLCache",
      "source_files": "kanzhunLib/kzurlcache/KZURLCache/KZURLCache/**/*.{h,m}",
      "dependencies": {
        "AFNetworking/NSURLSession": [

