{
  "name": "MultiAds",
  "version": "0.2.3",
  "summary": "A CocoaPods library written in Swift",
  "description": "This CocoaPods library helps you perform calculation.",
  "homepage": "https://gitlab.com/colddream/multiads",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Thang Do": "thangdq.uit.edu@gmail.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5",
  "source": {
    "git": "https://gitlab.com/colddream/multiads.git",
    "tag": "0.2.3"
  },
  "source_files": [
    "MultiAds/*.{h,swift}",
    "MultiAds/**/*.swift"
  ],
  "resource_bundles": {
    "MultiAds": [
      "MultiAds/**/*.{storyboard,xib}"
    ]
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xcc -Wno-error=non-modular-include-in-framework-module",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xcc -Wno-error=non-modular-include-in-framework-module",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": true,
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "dependencies": {
    "StartAppSDK": [
      "~> 4.7.1"
    ],
    "APDAdColonyAdapter": [
      "2.11.1.3"
    ],
    "APDAmazonAdsAdapter": [
      "2.11.1.2"
    ],
    "APDAppLovinAdapter": [
      "2.11.1.3"
    ],
    "APDBidMachineAdapter": [
      "2.11.1.3"
    ],
    "APDFacebookAudienceAdapter": [
      "2.11.1.2"
    ],
    "APDGoogleAdMobAdapter": [
      "2.11.1.3"
    ],
    "APDIronSourceAdapter": [
      "2.11.1.3"
    ],
    "APDMyTargetAdapter": [
      "2.11.1.2"
    ],
    "APDOguryAdapter": [
      "2.11.1.1"
    ],
    "APDUnityAdapter": [
      "2.11.1.2"
    ],
    "APDVungleAdapter": [
      "2.11.1.2"
    ],
    "APDYandexAdapter": [
      "2.11.1.1"
    ],
    "SnapKit": [
      "~> 5.6.0"
    ]
  },
  "static_framework": true,
  "swift_version": "5"
}
