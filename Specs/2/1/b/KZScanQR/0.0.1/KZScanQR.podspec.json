    "yangmaosheng": "yangmaosheng@kanzhun.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "git@git.kanzhun-inc.com:bosszhipin/kzscanqr.git",
    "tag": "0.0.1"
  },
