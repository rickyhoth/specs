{
  "name": "Ads-Global-Beta",
  "version": "4.2.0.0",
  "summary": "Ads-Global-Beta is a SDK from Bytedance providing union AD service.",
  "description": "Ads-Global-Beta provides ADs which include native、banner、RewardVideo、FullscreenVideo etc.",
  "license": {
    "type": "MIT",
    "file": "SDK/LICENSE"
  },
  "authors": {
    "bytedance-tech": "zywork@bytedance.com"
  },
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/Bytedance-UnionAD.git",
  "source": {
    "http": "https://sf16-fe-tos-sg.i18n-pglstatp.com/obj/pangle-sdk-static-va/4.2.0.0/SDK.zip"
  },
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": [
    "UIKit",
    "MapKit",
    "WebKit",
    "MediaPlayer",
    "CoreLocation",
    "AdSupport",
    "CoreMedia",
    "AVFoundation",
    "CoreTelephony",
    "StoreKit",
    "SystemConfiguration",
    "MobileCoreServices",
    "CoreMotion",
    "Accelerate",
    "AudioToolbox",
    "JavaScriptCore",
    "Security",
    "CoreImage",
    "AudioToolbox",
    "ImageIO",
    "QuartzCore",
    "CoreGraphics",
    "CoreText",
    "AppTrackingTransparency"
  ],
  "libraries": [
    "c++",
    "resolv",
    "z",
    "sqlite3",
    "bz2",
    "xml2",
    "iconv",
    "c++abi"
  ],
  "preserve_paths": "SDK/README.md",
  "default_subspecs": [
    "BUAdSDK",
    "APM"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "APM",
      "dependencies": {
        "RangersAPM-Pangle/Crash": [
          "2.3.2-pangle"
        ],
        "RangersAPM-Pangle/Global": [
          "2.3.2-pangle"
        ],
        "RangersAPM-Pangle/SessionTracker": [
          "2.3.2-pangle"
        ]
      }
    },
    {
      "name": "International",
      "preserve_paths": "SDK/BUVAAuxiliary.framework",
      "vendored_frameworks": [
        "SDK/BUVAAuxiliary.framework"
      ]
    },
    {
      "name": "Domestic",
      "vendored_frameworks": [
        "SDK/BUCNAuxiliary.framework"
      ],
      "preserve_paths": "SDK/BUCNAuxiliary.framework"
    },
    {
      "name": "BUAdSDK",
      "preserve_paths": "SDK/BUAdSDK.framework",
      "vendored_frameworks": [
        "SDK/BUAdSDK.framework"
      ],
      "dependencies": {
        "Ads-Global-Beta/BUFoundation": [

        ],
        "Ads-Global-Beta/International": [

        ]
      },
      "resources": "SDK/BUAdSDK.bundle"
    },
    {
      "name": "BUFoundation",
      "preserve_paths": "SDK/BUFoundation.framework",
      "vendored_frameworks": [
        "SDK/BUFoundation.framework"
      ]
    }
  ]
}
