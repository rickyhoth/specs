{
  "name": "Backtrace",
  "version": "2.0.0",
  "swift_versions": "5",
  "summary": "Backtrace's integration with iOS, macOS and tvOS",
  "description": "Reliable crash and hang reporting for iOS, macOS and tvOS.",
  "homepage": "https://backtrace.io/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Backtrace I/O": "info@backtrace.io"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/backtrace-cocoa.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.13",
    "tvos": "11.0"
  },
  "ios": {
    "source_files": [
      "Sources/**/*.{swift}",
      "Backtrace-iOS/**/*.{h*,swift}"
    ],
    "public_header_files": [
      "Backtrace-iOS/**/*.h*"
    ]
  },
  "osx": {
    "source_files": [
      "Sources/**/*.{swift}",
      "Backtrace-macOS/**/*.{h*,swift}"
    ],
    "public_header_files": [
      "Backtrace-macOS/**/*.h*"
    ]
  },
  "tvos": {
    "source_files": [
      "Sources/**/*.{swift}",
      "Backtrace-tvOS/**/*.{h*,swift}"
    ],
    "exclude_files": [
      "Sources/Features/Breadcrumb/**/*.{swift}"
    ],
    "public_header_files": [
      "Backtrace-tvOS/**/*.h*"
    ]
  },
  "dependencies": {
    "Backtrace-PLCrashReporter": [
      "1.5.3"
    ]
  },
  "resources": "Sources/**/*.xcdatamodeld",
  "swift_version": "5"
}
