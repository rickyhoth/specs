{
  "name": "Frames",
  "version": "3.5.0",
  "summary": "Checkout API Client, Payment Form UI and Utilities in Swift",
  "description": "Checkout API Client and Payment Form Utilities in Swift.\nThis library contains methods to implement a payment form as well as UI elements.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/frames-ios.git",
  "swift_versions": "5.0",
  "license": "MIT",
  "authors": {
    "Checkout.com Integration": "integration@checkout.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/frames-ios.git",
    "tag": "3.5.0"
  },
  "source_files": "Source/**/*.swift",
  "exclude_files": "Classes/Exclude",
  "resources": "Source/Resources/**/*",
  "dependencies": {
    "PhoneNumberKit": [
      "~> 3.3"
    ],
    "CheckoutEventLoggerKit": [
      "~> 1.0"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64 armv7 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/**/*.swift",
      "resources": "Tests/Fixtures/*",
      "requires_app_host": true
    }
  ],
  "swift_version": "5.0"
}
