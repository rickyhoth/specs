{
  "name": "xpjsip",
  "version": "2.12.1",
  "summary": "pjsip for iOS",
  "description": "pjsip for iOS.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/xpjsip",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "fubusli": "fubusili@126.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/xpjsip.git",
    "tag": "2.12.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "ios": {
    "source_files": "xpjsip/**/*.{h,hpp}",
    "vendored_libraries": "xpjsip/lib/**/*.a",
    "public_header_files": [
      "xpjsip/include/*.{h,hpp}",
      "xpjsip/include/**/*.{h,hpp}"
    ],
    "preserve_paths": "xpjsip/lib/**/*",
    "frameworks": [
      "AudioToolbox",
      "AVFoundation"
    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/Headers/Public/xpjsip/include\"",
    "GCC_PREPROCESSOR_DEFINITIONS": "PJ_AUTOCONF=1"
  },
  "libraries": "stdc++",
  "header_mappings_dir": "xpjsip",
  "requires_arc": false,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
