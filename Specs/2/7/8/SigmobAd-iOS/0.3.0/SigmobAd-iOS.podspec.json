{
  "name": "SigmobAd-iOS",
  "version": "0.3.0",
  "summary": "WindSDK is a SDK from Sigmob providing AD service.",
  "description": "SigmobAd-iOS provides ADs which include native、banner、splash、RewardVideo etc.",
  "homepage": "http://www.sigmob.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Codi": "codi.zhao@sigmob.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-lObjC"
    ]
  },
  "source": {
    "http": "https://sdkres.sigmob.cn/wind/ios/4.3.0_fcedec9d5439e0b6c2467cecd9cf17c7/wind_release_ios_4.3.0_20220830.zip"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "requires_arc": true,
  "default_subspecs": "WindSDK",
  "subspecs": [
    {
      "name": "WindSDK",
      "frameworks": [
        "ImageIO",
        "StoreKit",
        "CFNetwork",
        "CoreMedia",
        "AdSupport",
        "CoreMotion",
        "MediaPlayer",
        "CoreGraphics",
        "AVFoundation",
        "CoreLocation",
        "CoreTelephony",
        "SafariServices",
        "MobileCoreServices",
        "SystemConfiguration",
        "AudioToolbox"
      ],
      "weak_frameworks": [
        "WebKit",
        "UIKit",
        "Foundation"
      ],
      "libraries": [
        "c++",
        "z",
        "sqlite3"
      ],
      "vendored_frameworks": [
        "wind-sdk-ios/WindSDK.xcframework"
      ],
      "preserve_paths": "wind-sdk-ios/WindSDK.xcframework",
      "dependencies": {
        "SigmobAd-iOS/WindFoundation": [

        ]
      }
    },
    {
      "name": "WindFoundation",
      "vendored_frameworks": [
        "wind-sdk-ios/WindFoundation.xcframework"
      ],
      "preserve_paths": "wind-sdk-ios/WindFoundation.xcframework"
    }
  ]
}
