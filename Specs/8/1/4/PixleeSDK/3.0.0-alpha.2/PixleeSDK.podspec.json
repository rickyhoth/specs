{
  "name": "PixleeSDK",
  "version": "3.0.0-alpha.2",
  "summary": "An API Wrapper for Pixlee API",
  "description": "This SDK makes it easy for Pixlee customers to easily include Pixlee albums in their native iOS apps. It includes a native wrapper to the Pixlee album API as well as some drop-in and customizable UI elements to quickly get you started. This repo includes both the Pixlee iOS SDK and an example project to show you how it's used.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/pixlee-ios-sdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Sungjun Hong": "sungjun@pixleeteam.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/pixlee-ios-sdk.git",
    "tag": "3.0.0-alpha.2"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "default_subspecs": "SDK",
  "subspecs": [
    {
      "name": "SDK",
      "source_files": "Sources/PixleeSDK/**/*{swift}",
      "resources": "Sources/PixleeSDK/**/*.{png,jpeg,jpg,storyboard,xib,xcassets}",
      "dependencies": {
        "Alamofire": [
          "~> 5.0"
        ],
        "Nuke": [
          "~> 8.0"
        ],
        "Gifu": [
          "~> 3.3"
        ],
        "InfiniteLayout": [
          "~> 0.4.2"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
