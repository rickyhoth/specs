{
  "name": "EPush",
  "version": "1.0.0",
  "license": {
    "type": "Copyright",
    "text": "Hyphenate Inc. 2017"
  },
  "summary": "An Objective-C client for PUSH service",
  "homepage": "http://www.easemob.com/",
  "authors": {
    "Hyphenate Inc.": "sdk@easemob.com"
  },
  "source": {
    "http": "https://downloadsdk.easemob.com/downloads/EMPush_iOS/EMPush_1_0_0.zip"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "preserve_paths": "*.framework",
  "vendored_frameworks": "*.framework",
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "arm64 armv7 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
