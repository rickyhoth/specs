{
  "name": "PagPay",
  "version": "1.1.0",
  "summary": "Pagar com PagBank",
  "description": "O Pagar com PagBank é o produto do PagSeguro que permite aos vendedores utilizar o app PagBank como método de pagamento no checkout do seu próprio aplicativo. \nEste produto confere mais segurança aos vendedores devido aos padrões de validação de conta e cartões de crédito, adotados pelo PagBank.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/pagseguro-sdk-pagpay-ios",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE.txt"
  },
  "authors": {
    "PagSeguro Internet LTDA": "pagseguro.pd@uolinc.com"
  },
  "swift_versions": "5.4.2",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/pagseguro-sdk-pagpay-ios.git",
    "tag": "1.1.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "vendored_frameworks": "PagPay.xcframework",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited)",
    "ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES": "$(inherited)"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.4.2"
}
