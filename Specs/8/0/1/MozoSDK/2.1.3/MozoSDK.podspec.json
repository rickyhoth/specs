{
  "name": "MozoSDK",
  "version": "2.1.3",
  "summary": "Mozo protocol toolkit for Swift",
  "description": "The Mozo SDK is a Swift implementation of the Mozo protocol. It allows maintaining authentication, authorization with Mozo Services, receiving Mozo Tokens via beacons and buying, selling, transferring Mozo. It is also supporting UI components for authentication and buying, selling, transferring Mozo.",
  "homepage": "https://mozotoken.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Mozo Developers": "developer@mozocoin.io"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/mozo-ios-sdk.git",
    "tag": "v2.1.3"
  },
  "social_media_url": "https://www.facebook.com/mozoxvn",
  "swift_versions": "5",
  "platforms": {
    "ios": "12.0"
  },
  "source_files": "MozoSDK/**/*.{h,m,swift}",
  "resources": [
    "MozoSDK/Classes/**/*.xcdatamodeld",
    "MozoSDK/Classes/**/*.txt",
    "MozoSDK/Assets/**"
  ],
  "resource_bundles": {
    "MozoSDK": [
      "MozoSDK/Classes/**/*.{storyboard,xib}",
      "MozoSDK/Assets/*.{xcassets,gif}",
      "MozoSDK/Localization/*.lproj/*.strings"
    ]
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "dependencies": {
    "AppAuth": [

    ],
    "CoreStore": [

    ],
    "RNCryptor": [
      "~> 5.1.0"
    ],
    "web3swift": [
      "~> 2.5.0"
    ],
    "ReachabilitySwift": [

    ],
    "MBProgressHUD": [

    ],
    "Alamofire": [

    ],
    "SwiftyJSON": [

    ],
    "libPhoneNumber-iOS": [

    ],
    "JWTDecode": [

    ],
    "SDWebImage": [

    ]
  },
  "swift_version": "5"
}
