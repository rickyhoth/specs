{
  "name": "ADPhotoKit",
  "version": "1.1.1",
  "summary": "A library for select photos from album implemented by pure-Swift.",
  "description": "ADPhotoKit is a pure-Swift library to select assets (e.g. photo,video,gif,livephoto) from system album.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/ADPhotoKit.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "duzexu": "zexu007@qq.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/ADPhotoKit.git",
    "tag": "1.1.1"
  },
  "documentation_url": "https://duzexu.github.io/ADPhotoKit/",
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.1",
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "Photos",
    "PhotosUI",
    "AVFoundation"
  ],
  "default_subspecs": "CoreUI",
  "subspecs": [
    {
      "name": "Base",
      "source_files": [
        "ADPhotoKit/Classes/Base/**/*.swift"
      ],
      "preserve_paths": "ADPhotoKit/ADPhotoKit.docc",
      "resource_bundles": {
        "ADPhotoKitBase": [
          "ADPhotoKit/Assets/Base/**/*"
        ]
      }
    },
    {
      "name": "Core",
      "dependencies": {
        "ADPhotoKit/Base": [

        ],
        "Kingfisher": [
          "<= 5.15.8"
        ]
      },
      "source_files": [
        "ADPhotoKit/Classes/Core/**/*.swift"
      ],
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "Module_Core"
      }
    },
    {
      "name": "CoreUI",
      "dependencies": {
        "ADPhotoKit/Core": [

        ],
        "SnapKit": [

        ]
      },
      "source_files": [
        "ADPhotoKit/Classes/CoreUI/**/*.swift"
      ],
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "Module_UI"
      },
      "resource_bundles": {
        "ADPhotoKitCoreUI": [
          "ADPhotoKit/Assets/CoreUI/**/*"
        ]
      }
    },
    {
      "name": "ImageEdit",
      "dependencies": {
        "ADPhotoKit/Base": [

        ],
        "SnapKit": [

        ]
      },
      "source_files": [
        "ADPhotoKit/Classes/ImageEdit/**/*.swift"
      ],
      "pod_target_xcconfig": {
        "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "Module_ImageEdit"
      },
      "resource_bundles": {
        "ADPhotoKitImageEdit": [
          "ADPhotoKit/Assets/ImageEdit/**/*"
        ]
      }
    }
  ],
  "swift_version": "5.1"
}
