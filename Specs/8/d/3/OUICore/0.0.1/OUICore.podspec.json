      "~> 3.0"
    ],
    "ZLPhotoBrowser": [
      "4.3.0"
    ],
    "SVProgressHUD": [

    ],
    "IQKeyboardManagerSwift": [

    ],
    "Localize-Swift": [
      "~> 3.2"
    ],
    "Alamofire": [
      "4.9.1"
    ]
  },
  "swift_version": "5.0"
}
