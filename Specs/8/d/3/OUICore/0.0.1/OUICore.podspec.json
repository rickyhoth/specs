    "file": "LICENSE"
  },
  "authors": {
    "rentsoft": "https://www.rentsoft.cn/"
  },
  "source": {
    "git": "git@github.com:OIMUI/OUICore.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "source_files": "OUICore/Classes/**/*",
