{
  "name": "OktaLogger",
  "version": "1.3.5",
  "summary": "Logging proxy for standardized logging interface across products",
  "description": "Standard interface for all logging in Okta apps + SDK. Supports file, console, firebase logging destinations.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/okta-logger-swift",
  "license": {
    "type": "APACHE2",
    "file": "LICENSE"
  },
  "authors": {
    "Okta Developers": "developer@okta.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/okta-logger-swift.git",
    "tag": "1.3.5"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.14",
    "watchos": "6.0"
  },
  "swift_versions": "5.0",
  "dependencies": {
    "SwiftLint": [

    ]
  },
  "default_subspecs": "Complete",
  "static_framework": true,
  "subspecs": [
    {
      "name": "Complete",
      "dependencies": {
        "OktaLogger/FileLogger": [

        ],
        "OktaLogger/FirebaseCrashlytics": [

        ],
        "OktaLogger/InstabugLogger": [

        ]
      }
    },
    {
      "name": "MacOS",
      "platforms": {
        "osx": "10.14"
      },
      "dependencies": {
        "OktaLogger/FileLogger": [

        ],
        "OktaLogger/AppCenterLogger": [

        ]
      }
    },
    {
      "name": "FileLogger",
      "source_files": [
        "OktaLogger/FileLoggers/*.{h,m,swift}"
      ],
      "dependencies": {
        "CocoaLumberjack/Swift": [
          "~>3.6.0"
        ],
        "OktaLogger/Core": [

        ]
      }
    },
    {
      "name": "FirebaseCrashlytics",
      "source_files": [
        "OktaLogger/FirebaseCrashlyticsLogger/OktaLoggerFirebaseCrashlyticsLogger.swift"
      ],
      "dependencies": {
        "Firebase/Crashlytics": [
          "~> 8.0"
        ],
        "OktaLogger/Core": [

        ]
      }
    },
    {
      "name": "AppCenterLogger",
      "platforms": {
        "osx": "10.14"
      },
      "source_files": [
        "OktaLogger/AppCenterLogger/*"
      ],
      "dependencies": {
        "AppCenter": [
          "~>4.3.0"
        ],
        "OktaLogger/Core": [

        ]
      }
    },
    {
      "name": "InstabugLogger",
      "ios": {
        "source_files": [
          "OktaLogger/InstabugLogger/*"
        ],
        "dependencies": {
          "Instabug": [
            "~> 10.7"
          ],
          "OktaLogger/Core": [

          ]
        }
      }
    },
    {
      "name": "Core",
      "source_files": "OktaLogger/*.{h,m,swift}",
      "exclude_files": [
        "OktaLogger/Info.plist",
        "OktaLogger/FileLoggers",
        "OktaLogger/FirebaseCrashlyticsLogger",
        "OktaLogger/AppCenterLogger",
        "OktaLogger/InstabugLogger"
      ]
    }
  ],
  "swift_version": "5.0"
}
