{
  "name": "BXTencentOpenAPI",
  "version": "3.1.0",
  "summary": "Tencent Open API library for iOS to access Tencent platform.",
  "description": "It's a shame that Tencent dev team doesn't have an official pod for their Open API library.\nTherefore, we create this pod to track the official Tencent Open API SDK.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/BXTencentOpenAPI",
  "license": {
    "type": "Custom",
    "file": "LICENSE"
  },
  "authors": {
    "Yiming Tang": "yimingnju@gmail.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/BXTencentOpenAPI.git",
    "tag": "v3.1.0"
  },
  "social_media_url": "https://twitter.com/yiming_t",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "frameworks": [
    "Security",
    "SystemConfiguration",
    "CoreTelephony",
    "CoreGraphics"
  ],
  "libraries": [
    "iconv",
    "z",
    "c++",
    "sqlite3"
  ],
  "default_subspecs": "Basic",
  "subspecs": [
    {
      "name": "Basic",
      "vendored_frameworks": "TencentOpenAPI/Basic/TencentOpenAPI.framework"
    },
    {
      "name": "Advanced",
      "vendored_frameworks": "TencentOpenAPI/Advanced/TencentOpenAPI.framework",
      "resources": "TencentOpenAPI/Advanced/TencentOpenApi_IOS_Bundle.bundle"
    }
  ]
}
