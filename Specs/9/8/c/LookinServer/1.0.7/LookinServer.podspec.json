{
  "name": "LookinServer",
  "version": "1.0.7",
  "summary": "The iOS framework of Lookin.",
  "description": "Embed this framework into your iOS project to enable Lookin mac app.",
  "homepage": "https://lookin.work",
  "license": "GPL-3.0",
  "authors": {
    "Li Kai": "lookin@lookin.work"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0"
  },
  "default_subspecs": "Core",
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/LookinServer.git",
    "tag": "1.0.7"
  },
  "frameworks": "UIKit",
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Src/Core/**/*"
    },
    {
      "name": "Swift",
      "dependencies": {
        "LookinServer/Core": [

        ]
      },
      "source_files": "Src/Swift/**/*",
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "LOOKIN_SERVER_SWIFT_ENABLED=1"
      }
    }
  ]
}
