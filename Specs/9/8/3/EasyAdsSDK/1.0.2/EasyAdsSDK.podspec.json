        "EasyAdsSDK/Core": [

        ],
        "EasyAdsSDK/Adspot": [

        ],
        "GDTMobSDK": [

        ]
      },
      "source_files": "EasyAdsSDK/Adapter/GDT/**/*.{h,m}",
      "frameworks": [
        "AdSupport",
        "CoreLocation",
        "QuartzCore",
        "SystemConfiguration",
        "CoreTelephony",
        "Security",
        "StoreKit",
        "AVFoundation",
        "WebKit"
      ],
