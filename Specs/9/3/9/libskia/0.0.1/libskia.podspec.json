{
  "name": "libskia",
  "version": "0.0.1",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "libskia binding.",
  "homepage": "https://bitbucket.devops.nonprod.empf.local/scm/react/libskia",
  "authors": {
    "Satish Babariya": "satish.babariya@gmail.com"
  },
  "source": {
    "git": "https://bitbucket.devops.nonprod.empf.local/scm/react/libskia.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) SK_GL=1 SK_METAL=1"
  },
  "frameworks": [
    "GLKit",
    "MetalKit"
  ],
  "ios": {
    "vendored_libraries": [
      "libs/ios/x64/libskia.a",
      "libs/ios/x64/libsvg.a",
      "libs/ios/x64/libskshaper.a"
    ]
  },
  "header_mappings_dir": "skia",
  "source_files": "skia/**/*.{h,cpp}"
}
